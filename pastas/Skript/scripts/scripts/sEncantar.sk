command /exp1:
    trigger:
        if xp of player is more than 500:
            remove 500 from xp of player
            give 1 experience bottle named "&ePoção de experiência" with lore "&7XP: &f500" to player
            send "&aCompra feita com sucesso!"
        else:
            send "&cVocê não tem experiência suficiente"
