command /encantar:
	permission: enchant.vip
	permission message: &cVocê não tem permissão para executar este comando.
	trigger:
		set {xp.%player%} to xp of player
		open virtual chest inventory with size 3 named "Feiticeiro" to player
		create a gui slot 10 of player with experience bottle named "&ePoção de Experiência" with lore "&7Custo: &f500 XP||&7XP: &f500" to run player command "exp1" with left click
		create a gui slot 11 of player with experience bottle named "&ePoção de Experiência" with lore "&7Custo: &f1000 XP||&7XP: &f1000" to run player command "exp2" with left click
		create a gui slot 12 of player with experience bottle named "&ePoção de Experiência" with lore "&7Custo: &f5000 XP||&7XP: &f5000" to run player command "exp3" with left click
		create a gui slot 14 of player with book named "&eLivro de encantamentos" with lore "&7Tipo: &fSimples||&7Custo: &f1500 XP|| ||&7Botão esquerdo: &fComprar||&7Botão direito: &FVer encantamentos" to run player command "book1" with left click
		create a gui slot 14 of player with book named "&eLivro de encantamentos" with lore "&7Tipo: &fSimples||&7Custo: &f1500 XP|| ||&7Botão esquerdo: &fComprar||&7Botão direito: &FVer encantamentos" to run player command "simples" with right click
		create a gui slot 15 of player with book named "&eLivro de encantamentos" with lore "&7Tipo: &fNormal||&7Custo: &f3000 XP|| ||&7Botão esquerdo: &fComprar||&7Botão direito: &FVer encantamentos" to run player command "book2" with left click
		create a gui slot 15 of player with book named "&eLivro de encantamentos" with lore "&7Tipo: &fNormal||&7Custo: &f3000 XP|| ||&7Botão esquerdo: &fComprar||&7Botão direito: &FVer encantamentos" to run player command "normal" with right click
		create a gui slot 16 of player with book named "&eLivro de encantamentos" with lore "&7Tipo: &fAvançado||&7Custo: &f5500 XP|| ||&7Botão esquerdo: &fComprar||&7Botão direito: &FVer encantamentos" to run player command "book3" with left click
		create a gui slot 16 of player with book named "&eLivro de encantamentos" with lore "&7Tipo: &fAvançado||&7Custo: &f5500 XP|| ||&7Botão esquerdo: &fComprar||&7Botão direito: &FVer encantamentos" to run player command "avancado" with right click
		create a gui slot 26 of player with experience bottle named "&eSua experiência" with lore "&7XP: &f%{xp.%player%}%" to do nothing
command /simples:
	trigger:
